{
	"manifest_version": 2,
	"name": "__MSG_extensionName__",
	"description": "__MSG_extensionDescription__",
	"author": "Artur Pragacz",
	"homepage_url": "https://github.com/pragacz/window-arranger-webext",
	"version": "2.0.0b1",

	"default_locale": "en",

	"browser_specific_settings": {
		"gecko": {
			"id": "window_arranger@webext.pragacz.com",
			"strict_min_version": "67.0"
		}
	},

	"background": {
		"page": "background/background.html"
	},

	"browser_action": {
		"browser_style": true,
		"default_title": "Window Arranger",
		"default_popup": "popup/popup.html",
		"default_icon": {
			"16": "icons/16.png",
			"32": "icons/32.png",
			"48": "icons/48.png",
			"64": "icons/64.png"
		}
	},

	"options_ui": {
		"browser_style": true,
		"page": "settings/settings.html"
	},

	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"64": "icons/64.png",
		"96": "icons/96.png",
		"128": "icons/128.png"
	},

	"experiment_apis": {
		"windowsExtended": {
			"schema": "experiment/schemas/windowsExtended.json",
			"parent": {
				"scopes": [ "addon_parent" ],
				"paths": [ [ "windowsExt" ] ],
				"script": "experiment/windowsExtended.js"
			}
		}
	},

	"permissions": [
		"nativeMessaging",
		"storage",
		"sessions",
		"tabs",
		"<all_urls>"
	]
}
